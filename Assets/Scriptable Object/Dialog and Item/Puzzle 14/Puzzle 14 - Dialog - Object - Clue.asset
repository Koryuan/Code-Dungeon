  dialogs:
  - name: <color=#17e8d3><b>Tablet</b></color>
    text: Ada kumpulan kertas lagi disini
    m_image:
      Image: {fileID: 0}
      Width: 0
      Height: 0
      Y_Offset: 0
  - name: <color=#176be8><b>Player</b></color>
    text: Kenapa banyak sekali kertas disini
    m_image:
      Image: {fileID: 0}
      Width: 0
      Height: 0
      Y_Offset: 0
  - name: 
    text: '"Pengetahuan ku tidak dapat membantu ku lagi sekarang. Aku sudah melakukan
      apapun yang dapat ku lakukan untuk memperbaikinya, namun sekarang aku sudah
      kehabisan ide"'
    m_image:
      Image: {fileID: 0}
      Width: 0
      Height: 0
      Y_Offset: 0
  - name: 
    text: '"Rekan ku juga sudah menyerah, dan ingin segera membiarkan Subject017
      untuk mengurus sisanya. Subject017 sendiri masih dalam tahap pengembangan!"'
    m_image:
      Image: {fileID: 0}
      Width: 0
      Height: 0
      Y_Offset: 0
  - name: 
    text: '"Semua ini berantakan karena ulah dari para tester terkutuk. Kalau saja
      tidak ada mereka, aku dapat mela-"'
    m_image:
      Image: {fileID: 0}
      Width: 0
      Height: 0
      Y_Offset: 0
  - name: <color=#176be8><b>Player</b></color>
    text: Catatannya berhenti lagi
    m_image:
      Image: {fileID: 0}
      Width: 0
      Height: 0
      Y_Offset: 0
  - name: <color=#17e8d3><b>Tablet</b></color>
    text: Apa ini? Catatan harian? Kenapa terdengar dramatis sekali
    m_image:
      Image: {fileID: 0}
      Width: 0
      Height: 0
      Y_Offset: 0
  - name: <color=#176be8><b>Player</b></color>
    text: Jika tidak berguna sebaiknya tidak kita pikirkan
    m_image:
